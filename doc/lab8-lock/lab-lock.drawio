        <mxCell id="2rx-bC5pxt5CanDTIbP2-4" value="Timestamp to determine LRU in each bucket" style="shape=callout;whiteSpace=wrap;html=1;perimeter=calloutPerimeter;align=left;" vertex="1" parent="1">
